  "author": "Daydreamer",
  "license": "MIT",
  "dependencies": {
    "electron-store": "^6.0.1",
    "glasstron": "^0.0.6",
    "humanize-duration": "^3.25.1"
  },
  "devDependencies": {
    "electron": "^11.2.1",
    "electron-builder": "^22.9.1"
  }
}
